# Local development environment - calls real services
NODE_ENV=local
NEXT_PUBLIC_API_MODE=live
# .NET-based services (applications, roles, calendars, hierarchy)
NEXT_PUBLIC_BASE_URL=http://api.com
NEXT_PUBLIC_DOTNET_BASE_URL=http://api.com
# Java-based services (metadata management)
NEXT_PUBLIC_JAVA_BASE_URL=http://api-java.com
# File API services (for file downloads and uploads)
NEXT_PUBLIC_FILE_API_BASE_URL=http://api.com
NEXT_PUBLIC_FORCE_REAL_API=true
NEXT_PUBLIC_API_ENVIRONMENT=local